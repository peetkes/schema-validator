<?xml version="1.0" encoding="UTF-8"?>
<xs:schema elementFormDefault="qualified"
  targetNamespace="https://standaarden.overheid.nl/stop/imop/tekst/" version="2.0.0"
  xmlns:tekst="https://standaarden.overheid.nl/stop/imop/tekst/"
  xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:annotation>
    <xs:documentation source="https://koop.gitlab.io/lvbb/lvbb-ozon/go/goto.html?to=stop&amp;id=xsd:tekst">"Tekst-schema" van
      STOP.</xs:documentation>
    <xs:documentation>Dit schema definieert de "tekst"-elementen voor de
      STOP-modules</xs:documentation>
    <xs:documentation>versie 2.0.0</xs:documentation>
    <xs:documentation>Documentatie: https://koop.gitlab.io/lvbb/lvbb-ozon/</xs:documentation>
  </xs:annotation>

  <!--  ELEMENTEN -->
  <!-- ROOT REGELINGEN -->
  <xs:element name="RegelingKlassiek">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="tekst:RegelingOpschrift"/>
        <xs:element minOccurs="0" ref="tekst:Aanhef"/>
        <xs:element minOccurs="1" ref="tekst:Lichaam"/>
        <xs:element minOccurs="0" ref="tekst:Sluiting"/>
        <xs:group ref="tekst:mgBijlagenVoorRegeling"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agSchemaVersie"/>
      <xs:attributeGroup ref="tekst:agComponentNieuweRegeling"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="RegelingCompact">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="tekst:RegelingOpschrift"/>
        <xs:element ref="tekst:Lichaam"/>
        <xs:group ref="tekst:mgBijlagenVoorRegeling"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agSchemaVersie"/>
      <xs:attributeGroup ref="tekst:agComponentNieuweRegeling"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="RegelingTijdelijkdeel">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="tekst:RegelingOpschrift"/>
        <xs:element name="Lichaam">
          <xs:complexType>
            <xs:sequence>
              <xs:element ref="tekst:Conditie"/>
              <xs:group ref="tekst:mgLichaamStructuurTijdelijkdeel"/>
            </xs:sequence>
            <xs:attributeGroup ref="tekst:agAKN"/>
          </xs:complexType>
        </xs:element>
        <xs:group ref="tekst:mgBijlagenVoorRegeling"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agSchemaVersie"/>
      <xs:attributeGroup ref="tekst:agComponentNieuweRegeling"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="RegelingVrijetekst">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="tekst:RegelingOpschrift"/>
        <xs:element name="Lichaam">
          <xs:complexType>
            <xs:sequence>
              <xs:choice maxOccurs="unbounded">
                <xs:group ref="tekst:mgDivisieStructuur"/>
              </xs:choice>
            </xs:sequence>
            <xs:attributeGroup ref="tekst:agAKN"/>
          </xs:complexType>
        </xs:element>
        <xs:element maxOccurs="unbounded" minOccurs="0" ref="tekst:Bijlage"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agSchemaVersie"/>
      <xs:attributeGroup ref="tekst:agComponentNieuweRegeling"/>
    </xs:complexType>
  </xs:element>
  <!-- EINDE ROOT REGELINGEN -->

  <!-- ROOT BESLUITEN -->
  <xs:element name="BesluitKlassiek">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="tekst:RegelingOpschrift"/>
        <xs:element minOccurs="0" ref="tekst:Aanhef"/>
        <xs:element ref="tekst:RegelingKlassiek"/>
        <xs:element minOccurs="0" ref="tekst:Sluiting"/>
        <xs:group ref="tekst:mgBijlagenVoorBesluit"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agSchemaVersie"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="BesluitCompact">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="tekst:RegelingOpschrift"/>
        <xs:element minOccurs="0" ref="tekst:Aanhef"/>
        <xs:element ref="tekst:Lichaam"/>
        <xs:element minOccurs="0" ref="tekst:Sluiting"/>
        <xs:element maxOccurs="unbounded" minOccurs="0" ref="tekst:WijzigBijlage"/>
        <xs:group ref="tekst:mgBijlagenVoorBesluit"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agSchemaVersie"/>
    </xs:complexType>
  </xs:element>
  <!-- EINDE ROOT BESLUITEN -->

  <!-- BEGIN ROOT ANDERS -->
  <xs:element name="Kennisgeving">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="tekst:RegelingOpschrift"/>
        <xs:element name="Lichaam">
          <xs:complexType>
            <xs:choice maxOccurs="unbounded">
              <xs:sequence>
                <xs:group ref="tekst:mgDivisieStructuur"/>
              </xs:sequence>
            </xs:choice>
            <xs:attributeGroup ref="tekst:agAKN"/>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agSchemaVersie"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="Rectificatie">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="tekst:RegelingOpschrift"/>
        <xs:element maxOccurs="1" minOccurs="1" name="Lichaam">
          <xs:complexType>
            <xs:sequence>
              <xs:element maxOccurs="unbounded" minOccurs="1" ref="tekst:Rectificatietekst"/>
            </xs:sequence>
            <xs:attributeGroup ref="tekst:agAKN"/>
          </xs:complexType>
        </xs:element>
        <xs:element minOccurs="0" ref="tekst:Sluiting"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agSchemaVersie"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="Tekstrevisie">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="1" minOccurs="1" ref="tekst:RegelingMutatie"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agSchemaVersie"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="OfficielePublicatie">
    <xs:complexType>
      <xs:sequence>
        <xs:group ref="tekst:mgBladen"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agSchemaVersie"/>
    </xs:complexType>
  </xs:element>
  <!-- EINDE ROOT -->

  <!-- PUBLICATIES -->
  <xs:element name="Staatscourant">
    <xs:complexType>
      <xs:group ref="tekst:mgBlad"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="Staatsblad">
    <xs:complexType>
      <xs:group ref="tekst:mgBlad"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="Gemeenteblad">
    <xs:complexType>
      <xs:group ref="tekst:mgBlad"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="Provinciaalblad">
    <xs:complexType>
      <xs:group ref="tekst:mgBlad"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="Waterschapsblad">
    <xs:complexType>
      <xs:group ref="tekst:mgBlad"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="BladGR">
    <xs:complexType>
      <xs:group ref="tekst:mgBlad"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="Bladaanduiding">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="2" ref="tekst:Titelregel"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="Titelregel">
    <xs:complexType mixed="true">
      <xs:choice maxOccurs="unbounded" minOccurs="0">
        <xs:group ref="tekst:mgInlinesCompleet"/>
      </xs:choice>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!-- EINDE PUBLICATIES -->

  <!-- ELEMENTEN FORMEEL -->
  <xs:element name="Bijlage">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="tekst:Kop"/>
        <xs:choice maxOccurs="1" minOccurs="0">
          <xs:element maxOccurs="1" ref="tekst:Gereserveerd"/>
          <xs:element maxOccurs="1" ref="tekst:Vervallen"/>
        </xs:choice>
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" ref="tekst:InleidendeTekst"/>
          <xs:choice maxOccurs="unbounded" minOccurs="0">
            <xs:element ref="tekst:Divisie"/>
            <xs:element ref="tekst:Divisietekst"/>
          </xs:choice>
        </xs:sequence>
        <xs:element minOccurs="0" ref="tekst:Sluiting"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="Toelichting">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="1" minOccurs="0" ref="tekst:Kop"/>
        <xs:choice>
          <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" ref="tekst:InleidendeTekst"/>
            <xs:element maxOccurs="1" minOccurs="0" ref="tekst:AlgemeneToelichting"/>
            <xs:element maxOccurs="1" minOccurs="0" ref="tekst:ArtikelgewijzeToelichting"/>
          </xs:sequence>
          <xs:choice maxOccurs="unbounded" minOccurs="1">
            <xs:element ref="tekst:Divisie"/>
            <xs:element ref="tekst:Divisietekst"/>
          </xs:choice>
        </xs:choice>
        <xs:element minOccurs="0" ref="tekst:Sluiting"/>
        <xs:element maxOccurs="unbounded" minOccurs="0" ref="tekst:Bijlage"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="AlgemeneToelichting">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="1" ref="tekst:Kop"/>
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" ref="tekst:InleidendeTekst"/>
          <xs:choice maxOccurs="unbounded" minOccurs="1">
            <xs:element ref="tekst:Divisie"/>
            <xs:element ref="tekst:Divisietekst"/>
          </xs:choice>
        </xs:sequence>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="ArtikelgewijzeToelichting">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="1" ref="tekst:Kop"/>
        <xs:choice>
          <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" ref="tekst:InleidendeTekst"/>
            <xs:choice maxOccurs="unbounded" minOccurs="1">
              <xs:element ref="tekst:Divisie"/>
              <xs:element ref="tekst:Divisietekst"/>
            </xs:choice>
          </xs:sequence>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Conditie">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" minOccurs="1" ref="tekst:Artikel"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="Divisietekst">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="1" minOccurs="0" ref="tekst:Kop"/>
        <xs:choice maxOccurs="1" minOccurs="0">
          <xs:element maxOccurs="1" ref="tekst:Gereserveerd"/>
          <xs:element maxOccurs="1" ref="tekst:Vervallen"/>
        </xs:choice>
        <xs:element maxOccurs="1" minOccurs="0" ref="tekst:Inhoud"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="Divisie">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="1" ref="tekst:Kop"/>
        <xs:choice maxOccurs="1" minOccurs="0">
          <xs:element maxOccurs="1" ref="tekst:Gereserveerd"/>
          <xs:element maxOccurs="1" ref="tekst:Vervallen"/>
        </xs:choice>
        <xs:element maxOccurs="1" minOccurs="0" ref="tekst:InleidendeTekst"/>
        <xs:choice maxOccurs="unbounded" minOccurs="0">
          <xs:element ref="tekst:Divisie"/>
          <xs:element ref="tekst:Divisietekst"/>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="InleidendeTekst">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="1" minOccurs="0" ref="tekst:Kop"/>
        <xs:choice maxOccurs="unbounded" minOccurs="1">
          <xs:element ref="tekst:Al"/>
          <xs:element ref="tekst:Lijst"/>
          <xs:element ref="tekst:Figuur"/>
          <xs:element ref="tekst:Tussenkop"/>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAKN"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="Inhoud">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded" minOccurs="1">
        <xs:group ref="tekst:mgBlocksMinimaal"/>
        <xs:group ref="tekst:mgBlocksVolledig"/>
        <xs:element ref="tekst:Kadertekst"/>
      </xs:choice>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="Kadertekst">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="1" minOccurs="0" ref="tekst:Kop"/>
        <xs:choice maxOccurs="unbounded" minOccurs="1">
          <xs:group ref="tekst:mgBlocksMinimaal"/>
          <xs:group ref="tekst:mgBlocksVolledig"/>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
      <xs:attributeGroup ref="tekst:agAKN"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="Gereserveerd">
    <xs:complexType>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="Vervallen">
    <xs:complexType>
      <xs:attribute name="wijzigactie" type="tekst:wlWijzigactieBeperkt" use="optional"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="WijzigBijlage">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="1" minOccurs="1" ref="tekst:Kop"/>
        <xs:choice>
          <xs:element ref="tekst:RegelingMutatie"/>
          <xs:group ref="tekst:mgRegelingModellen"/>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="RegelingMutatie">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element ref="tekst:VoegToe"/>
        <xs:element ref="tekst:Vervang"/>
        <xs:element ref="tekst:VervangKop"/>
        <xs:element ref="tekst:Verwijder"/>
        <xs:element ref="tekst:VervangRegeling"/>
      </xs:choice>
      <xs:attributeGroup ref="tekst:agComponentMutatie"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="VervangRegeling">
    <xs:complexType>
      <xs:choice>
        <xs:group ref="tekst:mgRegelingModellen"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  
  <xs:element name="VervangKop">
    <xs:complexType>
      <xs:sequence>
        <xs:sequence minOccurs="1">
          <xs:element minOccurs="0" ref="tekst:Nummer"/>
          <xs:element minOccurs="0" ref="tekst:Wat"/>
        </xs:sequence>
        <xs:element ref="tekst:Kop"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agVervangKop"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="Verwijder">
    <xs:complexType>
      <xs:sequence>
        <xs:sequence>
          <xs:element minOccurs="0" ref="tekst:Nummer"/>
          <xs:element minOccurs="0" ref="tekst:Wat"/>
        </xs:sequence>
        <xs:choice>
          <xs:group ref="tekst:mgStructuurRegeling"/>
          <xs:element ref="tekst:Lichaam"/>
          <xs:element ref="tekst:Bijlage"/>
          <xs:element ref="tekst:Toelichting"/>
          <xs:element ref="tekst:AlgemeneToelichting"/>
          <xs:element ref="tekst:ArtikelgewijzeToelichting"/>
          <xs:element ref="tekst:WijzigArtikel"/>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agWat"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="VoegToe">
    <xs:complexType>
      <xs:sequence>
        <xs:sequence>
          <xs:element minOccurs="0" ref="tekst:Nummer"/>
          <xs:element minOccurs="0" ref="tekst:Wat"/>
        </xs:sequence>
        <xs:choice minOccurs="0">
          <xs:group ref="tekst:mgStructuurRegeling"/>
          <xs:element ref="tekst:Lichaam"/>
          <xs:element maxOccurs="unbounded" ref="tekst:Bijlage"/>
          <xs:element ref="tekst:Toelichting"/>
          <xs:element ref="tekst:AlgemeneToelichting"/>
          <xs:element ref="tekst:ArtikelgewijzeToelichting"/>
          <xs:element ref="tekst:WijzigArtikel"/>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agWijzigLocatie"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="Vervang">
    <xs:complexType>
      <xs:sequence>
        <xs:sequence>
          <xs:element minOccurs="0" ref="tekst:Nummer"/>
          <xs:element minOccurs="0" ref="tekst:Wat"/>
        </xs:sequence>
        <xs:choice>
          <xs:group ref="tekst:mgStructuurRegeling"/>
          <xs:element ref="tekst:Lichaam"/>
          <xs:element ref="tekst:Bijlage"/>
          <xs:element ref="tekst:Toelichting"/>
          <xs:element ref="tekst:AlgemeneToelichting"/>
          <xs:element ref="tekst:ArtikelgewijzeToelichting"/>
          <xs:element ref="tekst:WijzigArtikel"/>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agVervang"/>
      <xs:attributeGroup ref="tekst:agVerplaatsing"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="Wat">
    <xs:complexType mixed="true">
      <xs:choice maxOccurs="unbounded" minOccurs="0">
        <xs:group ref="tekst:mgInlinesCompleet"/>
      </xs:choice>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="Rectificatietekst">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="1" minOccurs="0" ref="tekst:Kop"/>
        <xs:choice maxOccurs="unbounded" minOccurs="0">
          <xs:element ref="tekst:Al"/>
          <xs:element ref="tekst:Lijst"/>
        </xs:choice>
        <xs:choice maxOccurs="1" minOccurs="0">
          <xs:element ref="tekst:RegelingMutatie"/>
          <xs:element ref="tekst:BesluitMutatie"/>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="BesluitMutatie">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element name="VoegToe">
          <xs:complexType>
            <xs:sequence>
              <xs:sequence>
                <xs:element minOccurs="0" ref="tekst:Nummer"/>
                <xs:element minOccurs="0" ref="tekst:Wat"/>
              </xs:sequence>
              <xs:choice>
                <xs:group ref="tekst:mgStructuurRegeling"/>
                <xs:element ref="tekst:Bijlage"/>
                <xs:element ref="tekst:Toelichting"/>
                <xs:element ref="tekst:AlgemeneToelichting"/>
                <xs:element ref="tekst:ArtikelgewijzeToelichting"/>
                <xs:group ref="tekst:mgBesluitMutatieEenheden"/>
              </xs:choice>
            </xs:sequence>
            <xs:attributeGroup ref="tekst:agVervang"/>
            <xs:attributeGroup ref="tekst:agWijzigLocatie"/>
          </xs:complexType>
        </xs:element>
        <xs:element name="Vervang">
          <xs:complexType>
            <xs:sequence>
              <xs:sequence>
                <xs:element minOccurs="0" ref="tekst:Nummer"/>
                <xs:element minOccurs="0" ref="tekst:Wat"/>
              </xs:sequence>
              <xs:choice>
                <xs:group ref="tekst:mgStructuurRegeling"/>
                <xs:element ref="tekst:Bijlage"/>
                <xs:element ref="tekst:Toelichting"/>
                <xs:element ref="tekst:AlgemeneToelichting"/>
                <xs:element ref="tekst:ArtikelgewijzeToelichting"/>
                <xs:group ref="tekst:mgBesluitMutatieEenheden"/>
                <xs:element ref="tekst:RegelingOpschrift"/>
              </xs:choice>
            </xs:sequence>
            <xs:attributeGroup ref="tekst:agVervang"/>
          </xs:complexType>
        </xs:element>
        <xs:element ref="tekst:Verwijder"/>
        <xs:element ref="tekst:VervangKop"/>
      </xs:choice>
      <xs:attribute name="componentnaam" type="tekst:dtComponentnaam" use="required"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="Kop">
    <xs:complexType>
      <xs:sequence>
        <xs:choice minOccurs="1">
          <xs:sequence>
            <xs:element ref="tekst:Label"/>
            <xs:element minOccurs="0" ref="tekst:Nummer"/>
            <xs:element minOccurs="0" ref="tekst:Opschrift"/>
          </xs:sequence>
          <xs:sequence>
            <xs:element ref="tekst:Nummer"/>
            <xs:element minOccurs="0" ref="tekst:Label"/>
            <xs:element minOccurs="0" ref="tekst:Opschrift"/>
          </xs:sequence>
          <xs:element minOccurs="1" ref="tekst:Opschrift"/>
        </xs:choice>
        <xs:element maxOccurs="1" minOccurs="0" ref="tekst:Subtitel"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="Label">
    <xs:complexType mixed="true">
      <xs:choice maxOccurs="unbounded" minOccurs="0">
